{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "moduleResolution": "node",
    "types": ["node"],
    "lib": ["esnext"],
    "outDir": "./dist",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "disableSourceOfProjectReferenceRedirect": false,
    "rootDir": "src"
  }
}
